<% if dof.object %>
<% dof.object.each do |dobj|  %>
<div class="row">
    <div class="col-sm-2">
        <img class="img-fluid img-thumbnail" src="https://dp.la/thumb/<%= dobj.object %>"/>
    </div>
    <div class="col-sm-10">
        <%= dobj.title %> - <td ><%= dobj.dataProvider %> - <%= dobj.description %>
    </div>
</div>
<div class="form-group row p-3">
    <% f.object.exhibit_digital_objects.where(digital_object_id: dobj.id).ids %>
        <%# <!--<div class="col-2">
        <%= f.label :exhibit_digital_objects_attributes[f.object.exhibit_digital_objects.where(digital_object_id: dobj.id).ids.first][:note], class: "col-form-label" %>-->
        <!--</div>--> %>
        <%= f.fields_for :exhibit_digital_objects, f.object.exhibit_digital_objects.where(digital_object_id: dobj.id) do |newlink| %>
             <%= newlink.label :note %>
             <%= newlink.text_field :note %>
        <% end %>
</div>
<% end %>
<% end %>